{
  "intents": {
    "build_app": {
      "description": "Intent to build an application",
      "keywords": ["build", "create", "develop", "make", "app", "application", "project"],
      "patterns": [
        "build\\s+(?:me\\s+)?a\\s+(\\w+)(?:\\s+app)?",
        "create\\s+(?:me\\s+)?a\\s+(\\w+)(?:\\s+app)?",
        "develop\\s+(?:me\\s+)?a\\s+(\\w+)(?:\\s+app)?"
      ],
      "examples": [
        "build me a todo app",
        "create a weather application",
        "develop a note taking app"
      ],
      "parameters": ["app_type"]
    },
    "build_website": {
